<%- include("partials/header"); -%> 
<section id="Main">
    <div class="container-fluid px-0 container-video">
        <video class="video" autoplay muted loop>
          <source src="/video/video-sp.mp4" type="video/mp4" />
        </video>
        <div class="row heading-main-left">
            <div class="col-xl-7">
                <div class="text-start px-5">
                <h6 style="color: #d08f32;"><span class="d-flex align-items-end"><img class="home-icon" src="/image/icon/home-icon.png"/>Real Estate Agency</span></h6>
                <h3>Find Your Future Dream Home By Us</h3>
            </div>
            </div>
        </div>
        <div class="scroll-downs">
